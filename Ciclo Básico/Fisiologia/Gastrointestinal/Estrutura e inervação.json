        {
            "id": 2986,
            "institution_id": 4,
            "description": "Qual o nervo do sistema nervoso parassimpático que inerva o trato gastrointestinal superior?",
            "difficulty": "easy",
            "year": "2020-01-01T00:00:00.000Z",
            "is_enabled": true,
            "created_at": "2020-07-10T03:11:45.000Z",
