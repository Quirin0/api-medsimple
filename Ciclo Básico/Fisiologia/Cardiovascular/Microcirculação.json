        {
            "id": 2965,
            "institution_id": 4,
            "description": "Com base nos dados fornecidos abaixo e na equação de Starling, calcule a taxa de filtração capilar:\n- Pressão oncótica capilar = 30 mmHg.\n- Pressão hidrostática capilar = 50 mmHg.\n- Pressão hidrostática do interstício = 5 mmHg.\n- Pressão coloidosmótica do interstício = 5 mmHg.\n- Coeficiente de filtração relativo ao leito capilar = 10 mL/min/mmHg.",
            "difficulty": "easy",
            "year": "2019-01-01T00:00:00.000Z",
            "is_enabled": true,
            "created_at": "2020-07-09T14:37:42.000Z",
            "updated_at": "2022-05-13T00:55:32.000Z",
            "user_id": 108,
            "is_residency": false,
