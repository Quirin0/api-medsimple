        {
            "id": 3861,
            "institution_id": 4,
            "description": "Qual é o passo limitante da velocidade da glicólise e que compromete definitivamente a molécula de glicose com a via?",
            "difficulty": "easy",
            "year": "2020-01-01T00:00:00.000Z",
            "is_enabled": true,
            "created_at": "2020-08-03T21:11:14.000Z",
            "updated_at": "2020-08-03T21:11:14.000Z",
            "user_id": 50,
            "is_residency": false,
            "institution": {
                "id": 4,
                "title": "MEDsimple",
                "state": null,
                "is_enabled": true,
