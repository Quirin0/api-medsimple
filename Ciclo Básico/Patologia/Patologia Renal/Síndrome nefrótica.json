        {
            "id": 747,
            "institution_id": 4,
            "description": "Qual a principal causa de síndrome nefrótica em crianças?",
            "difficulty": "easy",
            "year": "2020-01-01T00:00:00.000Z",
            "is_enabled": true,
            "created_at": "2020-05-29T03:57:24.000Z",
            "updated_at": "2020-05-29T03:57:24.000Z",
            "user_id": 108,
            "is_residency": false,
            "institution": {
                "id": 4,
                "title": "MEDsimple",
                "state": null,
                "is_enabled": true,
