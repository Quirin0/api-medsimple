            "alternatives": [
                {
                    "id": 39093,
                    "question_id": 9563,
                    "description": "É um bacilo de tamanho relativamente grande quando comparado com outras bactérias.",
                    "explanation": "Falso, doc! \n\n- Na verdade o bacilo de Koch é extremamente pequeno, comparativamente com outros tipos de bactérias. ",
                    "order": 0,
                    "correct": false,
                    "created_at": "2021-07-01T17:06:55.000Z",
